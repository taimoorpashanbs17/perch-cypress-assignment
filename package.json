{
  "name": "perchcypressautomationassignment",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "test": "cypress run",
    "open": "cypress open",
    "bdd": "cypress run --env TAGS='not @ignore'",
    "bdd:open": "cypress open --env TAGS='not @ignore'",
    "clear:reports": "rimraf cypress/reports",
    "test:headless": "cypress run --browser chrome --headless",
    "test:alltest": "cypress run --spec 'cypress/e2e/features/**/*.feature'",
    "test:home": "cypress run --spec 'cypress/e2e/features/**/homePage.feature'",
    "test:profile": "cypress run --spec 'cypress/e2e/features/**/profilePage.feature'",
    "test:cart": "cypress run --spec 'cypress/e2e/features/**/cartPage.feature'"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "cypress": "^14.5.1",
    "cypress-xpath": "^2.0.1",
    "esbuild": "^0.25.6"
  }
}
